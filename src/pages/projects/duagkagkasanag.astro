---
import BaseLayout from "../../layouts/BaseLayout.astro";
---

<BaseLayout>
  <!-- start section -->
  <section class="big-section top-space-margin pb-0">
    <div class="container">
      <div class="row">
        <div class="col-12 atropos" data-atropos>
          <div class="atropos-scale">
            <div class="atropos-rotate">
              <div class="atropos-inner" data-atropos-offset="3">
                <img src="/src/assets/images/duagkagkasanagproj1.webp" alt="" />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- end section -->

  <section class="pt-5">
    <div class="container">
      <div class="row row-cols-1 row-cols-lg-2 mb-9">
        <div class="col md-mb-30px text-center text-lg-start">
          <h5 class="alt-font mb-0 text-dark-gray fw-500 w-80 lg-w-100">
            Duag kag Kasanag
          </h5>
        </div>
        <div class="col last-paragraph-no-margin text-center text-lg-start">
          <p>
            Duag kag Kasanag: A Grant A Wish project is brought to life every
            Christmas season. It embodies the spirit of giving, hope, and
            community. It's a heartwarming initiative that seeks to fulfill the
            wishes and dreams of those in need, especially among underserved
            communities.
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- start section -->
  <section
    class="one-fourth-screen"
    data-parallax-background-ratio="0.5"
    style="background-image: url(/src/assets/images/duagkagkasanagproj10.webp)"
  >
  </section>
  <!-- end section -->
  <!-- start section -->
  <section class="overflow-hidden pb-0 pt-30px">
    <div class="container">
      <div class="row mb-50px sm-mb-30px">
        <div class="col-12">
          <img
            src="../src/assets/images/duagkagkasanagproj11.webp"
            class="w-100"
            alt=""
          />
        </div>
      </div>
      <div class="row row-cols-1 row-cols-lg-2 mb-9">
        <div class="col md-mb-30px text-center text-lg-start">
          <h5 class="alt-font mb-0 text-dark-gray fw-500 w-80 lg-w-100">
            DUAG KAG KASANAG
          </h5>
        </div>
        <div class="col last-paragraph-no-margin text-center text-lg-start">
          <p>
            At JCI Regatta, we will continue to make this holiday
            season a time of joy and generosity, lighting up hearts with love
            and compassion.
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-12">
          <img
            src="/src/assets/images/duagkagkasanagproj4.webp"
            class="w-100"
            alt=""
          />
        </div>
      </div>
    </div>
  </section>

  <!-- end section -->
  <!-- start section -->
  <section class="overflow-hidden pb-0">
    <div class="container-fluid">
      <div class="row">
        <div class="col-12 col-md-12">
          <div
            class="outside-box-right-60 outside-box-left-60 sm-outside-box-right-0 sm-outside-box-left-0"
          >
            <div
              class="swiper magic-cursor drag-cursor"
              data-slider-options='{ "slidesPerView": 1, "spaceBetween": 60, "loop": true, "autoplay": { "delay": 2000, "disableOnInteraction": false },  "pagination": { "el": ".slider-three-slide-pagination", "clickable": true, "dynamicBullets": true }, "navigation": { "nextEl": ".slider-three-slide-next", "prevEl": ".slider-three-slide-prev" }, "keyboard": { "enabled": true, "onlyInViewport": true }, "breakpoints": { "992": { "slidesPerView": 3 }, "768": { "slidesPerView": 3 }, "320": { "slidesPerView": 1 } }, "effect": "slide" }'
            >
              <div class="swiper-wrapper">
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj3.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj5.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj6.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj7.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj8.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
                <!-- start slider item -->
                <div class="swiper-slide">
                  <img
                    src="/src/assets/images/duagkagkasanagproj9.webp"
                    class="w-100"
                    alt=""
                  />
                </div>
                <!-- end slider item -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- end section -->
  <!-- start section -->
  <section>
    <div class="container">
      <div class="row row-cols-1 row-cols-md-3 text-center">
        <div class="col sm-mb-30px">
          <img src="/src/assets/images/duagkagkasanagproj12.webp" alt="" />
        </div>
        <div class="col sm-mb-30px">
          <img src="/src/assets/images/duagkagkasanagproj2.webp" alt="" />
        </div>
        <div class="col">
          <img src="/src/assets/images/duagkagkasanagproj13.webp" alt="" />
        </div>
      </div>
    </div>
  </section>
  <!-- end section -->

